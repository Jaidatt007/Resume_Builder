<?xml version="1.0" encoding="UTF-8"?>

<web-app>
  <servlet>
    <servlet-name>SubmitResume</servlet-name>
    <servlet-class>com.resume.controller.SubmitResumeServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SubmitResume</servlet-name>
    <url-pattern>/submitResume</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>ViewResume</servlet-name>
    <servlet-class>com.resume.controller.ViewResumeServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ViewResume</servlet-name>
    <url-pattern>/viewResume</url-pattern>
  </servlet-mapping>
</web-app>

